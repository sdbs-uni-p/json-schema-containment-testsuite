        "id": 11,
        "schema1": {
            "const": {
                "alpha": 1.1
            }
        },
        "schema2": {
            "$id": "https://test.json-schema.org/dynamic-ref-with-multiple-paths/main",
            "$defs": {
                "inner": {
                    "$id": "inner",
                    "$dynamicAnchor": "foo",
                    "title": "inner",
                    "additionalProperties": {
                        "$dynamicRef": "#foo"
                    }
                }
            },
            "not": {
                "if": {
                    "propertyNames": {
                        "pattern": "^[a-m]"
                    }
                },
                "then": {
                    "title": "any type of node",
                    "$id": "anyLeafNode",
                    "$dynamicAnchor": "foo",
                    "$ref": "main#/$defs/inner"
                },
                "else": {
                    "title": "integer node",
                    "$id": "integerNode",
                    "$dynamicAnchor": "foo",
                    "type": [
                        "object",
                        "integer"
                    ],
                    "$ref": "main#/$defs/inner"
                }
            }
        },
        "tests": {
            "s1SubsetEqOfs2": false
        }
